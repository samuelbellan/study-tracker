{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron:start": "cross-env NODE_ENV=development electron .",
    "electron:dev": "concurrently -k \"npm run dev\" \"wait-on http-get://localhost:5173 && npm run electron:start\"",
    "electron:build": "npm run build && electron-builder"
  },
  "dependencies": {
    "electron-updater": "^6.8.3",
    "lucide-react": "^0.575.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "socket.io-client": "^4.8.3"
  },
  "author": "User",
  "description": "Real-Time Study Tracker",
  "build": {
    "appId": "com.user.studytracker",
    "productName": "Study Tracker",
    "directories": {
      "output": "dist-electron"
    },
    "win": {
      "target": "nsis"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "samuelbellan",
        "repo": "study-tracker"
      }
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^40.6.1",
    "electron-builder": "^26.8.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "vite": "^7.3.1",
    "wait-on": "^9.0.4"
  }
}